{
  "framework": null,
   "builds": [{
        "src": "WebAssignment/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb", "runtime":  "python3.10" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "WebAssignment/wsgi.py"
        }
    ]
}